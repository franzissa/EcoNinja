<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.econinja.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_shared" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.econinja.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/setting"
        android:name="com.example.econinja.setting"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />
    <fragment
        android:id="@+id/info2"
        android:name="com.example.econinja.Info"
        android:label="fragment_info"
        tools:layout="@layout/fragment_info" />
    <fragment
        android:id="@+id/shared"
        android:name="com.example.econinja.shared"
        android:label="fragment_shared"
        tools:layout="@layout/fragment_shared" />
</navigation>